num1 = int(input("Digite o primeiro numero: "))
num2 = int(input("Digite o segundo numero "))

soma = num1 + num2
sub = num1 - num2 
mult = num1 * num2
div = num1 / num2

print("O resultado da soma e:", soma, " \nO da subtração e:", sub, " \nO da multiplicação e:", mult, " \nE o da divisão e:", div,)
